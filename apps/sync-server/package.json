{
  "name": "@sandpaper/sync-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/start-server.ts",
    "build": "tsup src/start-server.ts --format esm --dts --out-dir dist",
    "start": "node dist/start-server.js",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.1",
    "hono": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.3.0",
    "eslint": "^9.39.2",
    "tsx": "^4.20.2",
    "tsup": "^8.5.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
